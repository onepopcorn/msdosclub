{
    "name": "msdos",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@shoelace-style/shoelace": "*",
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "classnames": "^2.3.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "^5.0.0",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-app-rewired start --scripts-version react-scripts",
        "build": "react-app-rewired build --scripts-version react-scripts",
        "test": "react-app-rewired test --scripts-version react-scripts --transformIgnorePatterns \"node_modules/(?!@shoelace-style)/\" --env=jsdom",
        "lint": "eslint ./src",
        "eject": "react-scripts eject",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "babel-jest": "^27.5.1",
        "copy-webpack-plugin": "^10.2.4",
        "husky": "^7.0.4",
        "prettier": "2.5.1",
        "react-app-rewired": "^2.1.11"
    }
